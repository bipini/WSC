<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Admin Home</title>
<link href="wscui/css/style.css"  rel="stylesheet">
 <link rel="stylesheet" href="wscui/css/bootstrap.min.css">

<script src="wscui/js/jquery.js"></script>
<script src="wscui/js/bootstrap.min.js"></script>
</head>
<body>

	<div id="main-warper" class="container-full">
	
	<table>		
			<tr>
				<td colspan="4" align="left">
					<a href="admin-user">Add User</a>
				</td>
			</tr>
			<c:forEach items="${users}" var="user">
				
				<tr>
					<td><c:out value="${user.userId}" /></td>
					<td><c:out value="${user.userName}" /></td>
					<td><c:out value="${user.email}" /></td>
					<td><a href="admin-user/${user.userId}">Edit</a></td>
				</tr>
				
			</c:forEach>		
	</table>
	</div>
	
</body>
</html>